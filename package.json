{
  "name": "renumerator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "npm run smui-theme",
    "smui-theme": "smui-theme compile static/smui.css",
    "copy-css": "cp static/smui.css dist/",
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "build-prod": "vite build --config vite.config-prod.js & npm run copy-css",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@smui/select": "^7.0.0-beta.15",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.25.1",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@types/node": "^20.7.0",
    "smui-theme": "^7.0.0-beta.15",
    "svelte": "^4.2.1",
    "svelte-check": "^3.4.3",
    "svelte-kit": "^1.2.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module"
}
